    "@types/serialize-javascript": "^5.0.2",
    "@vite-pwa/nuxt": "^0.0.7",
    "@vue-macros/nuxt": "^1.2.8",
    "@vue/compiler-sfc": "^3.2.47",
    "@vueuse/components": "^10.0.0",
    "@vueuse/core": "^10.0.0",
